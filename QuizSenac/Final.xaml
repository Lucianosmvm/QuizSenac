<Page x:Class="QuizSenac.Final"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:QuizSenac"
      mc:Ignorable="d" 
      Title="Final">

    <Grid>
        <Image x:Name="img_final"
               Source="/Quiz Senac 2025 (9).png"
               Stretch="UniformToFill"
               VerticalAlignment="Stretch"
               HorizontalAlignment="Stretch"/>
        <Button x:Name="btn_Sair" Content="Sair" HorizontalAlignment="Center" Margin="0,935,0,0" VerticalAlignment="Top" FontSize="36" FontWeight="Bold" Click="btn_Sair_Click"/>
       
        <Grid x:Name="gr_total" HorizontalAlignment="Center" Height="1080" VerticalAlignment="Center" Width="1920" Background="White">
            <Label x:Name="lb_total" Content="Label" HorizontalAlignment="Center" VerticalAlignment="Center" Height="152" Width="340" FontSize="90" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold"/>
        </Grid>
        
    </Grid>

</Page>
